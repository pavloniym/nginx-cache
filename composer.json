{
  "name": "pavloniym/nginx-cache",
  "description": "nginx cache package for routes",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Pavloniym\\NginxCache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pavloniym\\NginxCache\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pavloniym\\NginxCache\\NginxCacheServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.5"
  },
  "scripts": {
    "test": "phpunit --testdox"
  }
}
